### Copy this file to `.env` and fill in the secrets before running Docker Compose.

# PostgreSQL credentials for the metadata database
POSTGRES_USER=airflow
POSTGRES_PASSWORD=changeme
POSTGRES_DB=airflow

# Airflow secrets
# Generate a 32-byte JWT secret; e.g. `openssl rand -hex 32`
AIRFLOW_JWT_SECRET=replace-with-a-secure-jwt-secret
